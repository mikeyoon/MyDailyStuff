{
  "name": "MyDailyStuff.js",
  "version": "1.0.0",
  "description": "Frontend web application to MyDailyStuff",
  "scripts": {
    "postinstall": "(./node_modules/.bin/jspm install && ./node_modules/.bin/gulp deploy)",
    "start": "./node_modules/.bin/gulp watch",
    "jspm": "./node_modules/.bin/jspm install"
  },
  "devDependencies": {
    "bootstrap": "3.3.6",
    "fbjs": "^0.8.1",
    "gulp": "^3.9.0",
    "gulp-cachebust": "0.0.6",
    "gulp-less": "^3.0.3",
    "gulp-minify": "0.0.9",
    "gulp-nop": "0.0.3",
    "gulp-shell": "^0.5.2",
    "gulp-sourcemaps": "^1.5.2",
    "gulp-typescript": "^2.13.0",
    "gulp-watch": "^4.3.5",
    "jest-cli": "^11.0.1",
    "jspm": "^0.16.33",
    "react": "^15.0.1",
    "react-addons-test-utils": "^15.0.1",
    "react-date-picker": "^4.0.10",
    "react-dom": "^15.0.1",
    "systemjs-builder": "^0.15.15",
    "typescript": "^1.8.10"
  },
  "jspm": {
    "dependencies": {
      "bootstrap": "github:twbs/bootstrap@3.3.6",
      "classnames": "npm:classnames@2.2.3",
      "fluxxor": "npm:fluxxor@1.7.3",
      "jquery": "npm:jquery@2.2.3",
      "lodash": "npm:lodash@4.11.1",
      "marked": "npm:marked@0.3.5",
      "moment": "npm:moment@2.12.0",
      "page": "npm:page@1.7.1",
      "react": "npm:react@15.0.1",
      "react-date-picker": "npm:react-date-picker@4.0.10",
      "react-dom": "npm:react-dom@15.0.1",
      "rest": "github:cujojs/rest@1.3.2"
    },
    "devDependencies": {},
    "overrides": {
      "npm:react@15.0.1": {
        "main": "dist/react"
      },
      "npm:react-dom@15.0.1": {
        "main": "dist/react-dom"
      }
    }
  },
  "jest": {
    "scriptPreprocessor": "./jest-preprocessor.js",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/jspm_packages"
    ],
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/fbjs",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "ts",
      "tsx"
    ],
    "modulePathIgnorePatterns": [
      "jspm_packages/npm/isarray@0\\.0\\.1/"
    ],
    "testFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "testPathDirs": [
      "src"
    ]
  }
}
